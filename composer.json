        "OSL-3.0",
        "AFL-3.0"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
			"php-http/discovery": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "laminas/laminas-dependency-plugin": true,
            "magento/*": true
        }
    },
    "require": {
        "bsscommerce/custom-options-package": "^1.0",
        "bsscommerce/gallery": "^1.1",
        "bsscommerce/product-label": "^1.1",
        "chriskonnertz/string-calc": "dev-main",
        "cweagans/composer-patches": "^1.7",
        "datafeedwatch/dfwconnector-magento2": "dev-master",
        "firebase/php-jwt": "^6.3",
        "fooman/emailattachments-m2": "3.6.1",
        "fooman/sameorderinvoicenumber-m2": "^3.0",
        "mage2pro/core": "*",
        "magefan/module-seo-extra": "^2.5.1",
        "magemojo/m2-ce-cron": "^1.3",
        "magento/composer-dependency-version-audit-plugin": "~0.1",
        "magento/composer-root-update-plugin": "^2.0.4",
        "magento/product-community-edition": "2.4.8-p3",
        "magento/quality-patches": "^1.1",
        "mageplaza/magento-2-social-login": "^4.1",
        "mageplaza/module-smtp": "^4.7",
        "magepow/categories": "^1.0",
        "mageworx/module-shippingrulesmeta": "*",
        "meta/meta-for-magento2": "^1.3",
        "sansec/magento2-module-shield": "dev-main",
        "securetrading/stpp_json": "*",
        "stape/gtm-server-side-magento-module": "^1.0",
        "stripe/stripe-payments": "*",
        "trust-payments-limited/module-trust": "^4.0",
        "wyomind/simplegoogleshopping": "16",
        "yotpo/module-yotpo-reviews": "*"
    },
    "require-dev": {
        "allure-framework/allure-phpunit": "^3.1",
		"carlos-mg89/oauth": "^0.8.17",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7 || ^1.0",
        "dg/bypass-finals": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.22",
        "lusitanian/oauth": "^0.8",
		"magento/magento-allure-phpunit": "^3.0.2",
        "magento/magento-coding-standard": "*",
        "magento/magento2-functional-testing-framework": "^5.0",
        "pdepend/pdepend": "3.x-dev",
        "phpmd/phpmd": "3.x-dev",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10.5",
        "symfony/finder": "^6.4"
	},
    "autoload": {
        "psr-4": {
            "Magento\\Setup\\": "setup/src/Magento/Setup/"
        },
        "psr-0": {
            "": [
                "app/code/",
                "generated/code/"
            ]
        },
        "files": [
            "app/etc/NonComposerComponentRegistration.php"
        ],
        "exclude-from-classmap": [
            "**/dev/**",
            "**/update/**",
            "*/*/Test/**/*Test"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Magento\\PhpStan\\": "dev/tests/static/framework/Magento/PhpStan/",
            "Magento\\Sniffs\\": "dev/tests/static/framework/Magento/Sniffs/",
            "Magento\\TestFramework\\Inspection\\": "dev/tests/static/framework/Magento/TestFramework/Inspection/",
            "Magento\\TestFramework\\Utility\\": "dev/tests/static/framework/Magento/TestFramework/Utility/",
            "Magento\\Tools\\": "dev/tools/Magento/Tools/",
            "Magento\\Tools\\Sanity\\": "dev/build/publication/sanity/Magento/Tools/Sanity/"
        }
    },
    "version": "2.4.8-p3",
    "minimum-stability": "stable",
    "repositories": {
        "Magento": {
			"canonical": false,
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "magefan-seo-extra-m2": {
            "type": "composer",
            "url": "https://magefan.com/repo/9bapBUYj7cFl1SbC5u01BJ2WzgFWyxnz/KJ5fbb1DO7cUrRUZp1NHvsM6vX2VmL0h/"
        },
        "magearray": {
            "type": "composer",
            "url": "https://repo.magearray.com"
        },
        "wyomind": {
            "type": "composer",
            "url": "https://jon_pegasus_gb_net:HH78fdtj64efTRF478@repo.wyomind.com"
        },
        "bsscommerce": {
            "type": "composer",
            "url": "https://composer.bsscommerce.com/community"
        },
        "mirasvit-shr2": {
            "type": "composer",
            "url": "https://54653:C4V8E3ZSIW@packages.mirasvit.com/54653:C4V8E3ZSIW/"
        },
        "mageplaza": {
            "type": "composer",
            "url": "https://repo.mageplaza.com"
        },
        "mageworx_packages": {
            "type": "composer",
            "url": "https://packages.mageworx.com/"
        }
    },
    "extra": {
        "magento-force": "override"
    }
}
